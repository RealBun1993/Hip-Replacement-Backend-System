{
  "version": 2,
  "builds": [
    {
      "src": "api/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/predict_kidney",
      "dest": "/api/api.py",
      "methods": ["POST"]
    },
    {
      "src": "/api/predict_blood",
      "dest": "/api/api.py",
      "methods": ["POST"]
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.8.19"
  },
  "functions": {
    "api/api.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
