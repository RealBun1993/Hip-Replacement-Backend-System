{
  "version": 2,
  "builds": [
    {
      "src": "api/api.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.8" }
    }
  ],
  "routes": [
    {
      "src": "/api/predict_kidney",
      "dest": "/api/api.py",
      "methods": ["POST"]
    },
    {
      "src": "/api/predict_blood",
      "dest": "/api/api.py",
      "methods": ["POST"]
    }
  ],
  "functions": {
    "api/api.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
